<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true"><shape>
        <solid android:color="@color/selector" />
        <stroke android:width="1dp" android:color="#6d89a4" />
    </shape></item>
    <item android:state_checked="false"><shape android:shape="rectangle">
        <solid android:color="#fff6f6" />
        <stroke android:width="1dp" android:color="#7c7e81" />
    </shape></item>
</selector>